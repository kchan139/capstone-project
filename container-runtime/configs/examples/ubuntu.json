{
  "root": {
    "path": "/var/lib/mrunc/images/ubuntu",
    "readonly": false
  },
  "process": {
    "args": ["/bin/bash"],
    "env": [
    ],
    "cwd": "/",
    "terminal": true,
    "user": {"uid":0, "gid":0}
  },
  "linux": {
    "resources": {
      "cpu": {
        "shares": 1024,
        "quota": 50000,
        "period": 100000
      },
      "memory": {
        "limit": 536870912,
        "reservation": 536870912,
        "swap": 536870912
      },
      "pids": {
        "limit": 100
      }
    },
    "network": {
      "enableNetwork": true,
      "containerIP": "10.0.0.2/24",
      "gatewayIP": "10.0.0.1",
      "vethHost": "veth-mrunc-0",
      "vethContainer": "ceth0",
      "dns": ["8.8.8.8", "8.8.4.4", "1.1.1.1"],
      "firewallScript": "./configs/firewall-setup.sh"
    }
  }
}
